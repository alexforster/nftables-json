        "match": {
            "left": {
                "payload": {
                    "field": "saddr",
                    "protocol": "ip"
                }
            },
	    "op": "==",
            "right": {
                "set": [
                    "1.1.1.0",
                    "2.2.2.0",
                    "3.3.3.0"
                ]
            }
        }
    }
]


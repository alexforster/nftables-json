# dnat ip to ip saddr . tcp dport map { 192.168.1.2 . 80 : 10.141.10.2 . 8888 - 8999 }
[
    {
	"dnat": {
	    "addr": {
		"map": {
		    "data": {
			"set": [
			    [
				{
				    "concat": [
					"192.168.1.2",
					80
				    ]
				},
				{
				    "concat": [
					"10.141.10.2",
					{
					    "range": [
						8888,
						8999
					    ]
					}
				    ]
				}
			    ]
			]
		    },
		    "key": {
			"concat": [
			    {
				"payload": {
				    "field": "saddr",
				    "protocol": "ip"
				}
			    },
			    {
				"payload": {
				    "field": "dport",
				    "protocol": "tcp"
				}
			    }
			]
		    }
		}
	    },
	    "family": "ip"
	}
    }
]


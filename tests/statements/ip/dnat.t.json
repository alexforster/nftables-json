# iifname "eth0" tcp dport 80-90 dnat to 192.168.3.2
[
    {
        "match": {
            "left": {
                "meta": { "key": "iifname" }
            },
	    "op": "==",
            "right": "eth0"
        }
    },
    {
        "match": {
            "left": {
                "payload": {
                    "field": "dport",
                    "protocol": "tcp"
                }
            },
	    "op": "==",
            "right": {
                "range": [ 80, 90 ]
            }
        }
    },
    {
        "dnat": {
            "addr": "192.168.3.2"
        }
    }
]


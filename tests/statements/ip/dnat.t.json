                            {
                                "payload": {
                                    "field": "daddr",
                                    "protocol": "ip"
                                }
                            }
                        ]
                    },
                    "data": {
                        "set": [
                            [
                                {
                                    "concat": [
                                        "0x00000014",
                                        "1.1.1.1"
                                    ]
                                },
                                "1.2.3.4"
                            ]
                        ]
                    }
                }
            }
        }
    }
]


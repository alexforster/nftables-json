# iifname "eth0" ct state established,new tcp dport vmap {22 : drop, 222 : drop } redirect
[
    {
        "match": {
            "left": {
                "meta": { "key": "iifname" }
            },
	    "op": "==",
            "right": "eth0"
        }
    },
    {
        "match": {
            "left": {
                "ct": {
                    "key": "state"
                }
            },
	    "op": "in",
            "right": [
                "established",
                "new"
            ]
        }
    },
    {
        "vmap": {
            "key": {
                "payload": {
                    "field": "dport",
                    "protocol": "tcp"
                }
            },
            "data": {
                "set": [
                    [
                        22,
                        {
                            "drop": null
                        }
                    ],
                    [
                        222,
                        {
                            "drop": null
                        }
                    ]
                ]
            }
        }
    },
    {
        "redirect": null
    }
]

# ip protocol 6 redirect to :tcp dport map { 22 : 8000, 80 : 8080}

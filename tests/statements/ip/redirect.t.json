        "match": {
            "left": {
                "payload": {
                    "field": "protocol",
                    "protocol": "ip"
                }
            },
	    "op": "==",
            "right": 6
        }
    },
    {
        "redirect": {
            "port": {
                "map": {
                    "key": {
                        "payload": {
                            "field": "dport",
                            "protocol": "tcp"
                        }
                    },
                    "data": {
                        "set": [
                            [
                                22,
                                8000
                            ],
                            [
                                80,
                                8080
                            ]
                        ]
                    }
                }
            }
        }
    }
]


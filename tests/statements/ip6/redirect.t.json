# iifname "eth0" ct state established,new tcp dport vmap {22 : drop, 222 : drop } redirect
[
    {
        "match": {
            "left": {
                "meta": { "key": "iifname" }
            },
	    "op": "==",
            "right": "eth0"
        }
    },
    {
        "match": {
            "left": {
                "ct": {
                    "key": "state"
                }
            },
	    "op": "in",
            "right": [
                "established",
                "new"
            ]
        }
    },
    {
        "vmap": {
            "key": {
                "payload": {
                    "field": "dport",
                    "protocol": "tcp"
                }
            },
            "data": {
                "set": [
                    [
                        22,
                        {
                            "drop": null
                        }
                    ],
                    [
                        222,
                        {
                            "drop": null
                        }
                    ]
                ]
            }
        }
    },
    {
        "redirect": null
    }
]

